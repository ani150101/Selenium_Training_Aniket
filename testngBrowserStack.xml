<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="BrowserStackSuite" thread-count="5" parallel="tests">
	<test name="Test1">
		<parameter name="os" value="Windows"></parameter>
		<parameter name="osVersion" value="11"></parameter>
		<parameter name="browserName" value="Chrome"></parameter>
		<parameter name="browserVersion" value="107"></parameter>
		<classes>
			<class name="Testing.TestingBrowserStack" />
		</classes>
	</test>
	<test name="Test2">
		<parameter name="os" value="Windows"></parameter>
		<parameter name="osVersion" value="11"></parameter>
		<parameter name="browserName" value="Firefox"></parameter>
		<parameter name="browserVersion" value="105"></parameter>
		<classes>
			<class name="Testing.TestingBrowserStack" />
		</classes>
	</test>
	<test name="Test3">
		<parameter name="os" value="Windows"></parameter>
		<parameter name="osVersion" value="10"></parameter>
		<parameter name="browserName" value="Edge"></parameter>
		<parameter name="browserVersion" value="100"></parameter>
		<classes>
			<class name="Testing.TestingBrowserStack" />
		</classes>
	</test>
	<test name="Test4">
		<parameter name="os" value="OS X"></parameter>
		<parameter name="osVersion" value="Ventura"></parameter>
		<parameter name="browserName" value="Chrome"></parameter>
		<parameter name="browserVersion" value="102"></parameter>
		<classes>
			<class name="Testing.TestingBrowserStack" />
		</classes>
	</test>
	<test name="Test5">
		<parameter name="os" value="OS X"></parameter>
		<parameter name="osVersion" value="Ventura"></parameter>
		<parameter name="browserName" value="Firefox"></parameter>
		<parameter name="browserVersion" value="100"></parameter>
		<classes>
			<class name="Testing.TestingBrowserStack" />
		</classes>
	</test>
</suite> <!-- Suite -->
